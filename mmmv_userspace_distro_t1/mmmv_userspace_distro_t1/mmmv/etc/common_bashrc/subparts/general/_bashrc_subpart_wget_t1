#!/usr/bin/env bash
#==========================================================================
# Initial author of this file: Martin.Vahi@softf1.com
# This file is in public domain.
#
# The following line is a spdx.org license label line:
# SPDX-License-Identifier: 0BSD
#
# As exiting from the ~/.bashrc exits the Bash session,
# the ~/.bashrc and its subparts must not call "exit".
# This script and all of the functions defined in it are "exit" free.
#==========================================================================
# Example code for inclusion to the ~/.bashrc
#
#     S_FP_0="$HOME/<path to this script here>"
#     if [ -e "$S_FP_0" ]; then
#         if [ -d "$S_FP_0" ]; then
#             echo ""
#             echo "A folder with the path of "
#             echo ""
#             echo "    S_FP_0==$S_FP_0"
#             echo ""
#             echo "exists, but a file is expected."
#             echo "GUID=='281c3fd2-ebf4-446a-a308-5392805096e7'"
#             echo ""
#         else
#             source "$S_FP_0"
#         fi
#     else
#         echo ""
#         echo "~/.bashrc sub-part with the path of "
#         echo ""
#         echo "    S_FP_0==$S_FP_0"
#         echo ""
#         echo "could not be found."
#         echo "GUID=='740fd89f-2521-41f0-9808-5392805096e7'"
#         echo ""
#     fi
#
#--------------------------------------------------------------------------

func_mmmv_bashrc_subpart_wget_t1_error_message_t1(){
    local S_GUID_CANDIDATE=$1 # first  function argument
    #----------------------------------------------------------------------
    local SB_THIS_FUNCTION_IS_MISUSED="f"
    if [ "$SB_WGET_EXISTS_ON_PATH" == "t" ]; then
        SB_THIS_FUNCTION_IS_MISUSED="t"
    else
        if [ "$SB_WGET_EXISTS_ON_PATH" == "f" ]; then
            SB_THIS_FUNCTION_IS_MISUSED="t"
        fi
    fi
    #----------------------------------------------------------------------
    if [ "$SB_THIS_FUNCTION_IS_MISUSED" == "f" ]; then
        echo ""
        echo -e "\e[31mThe ~/.bashrc or at least one of its sub-parts has a flaw.\e[39m"
        echo "A valid domain for the SB_WGET_EXISTS_ON_PATH is {"t","f"}, but "
        echo ""
        echo "    SB_WGET_EXISTS_ON_PATH==\"$SB_WGET_EXISTS_ON_PATH\""
        echo ""
        if [ "$S_GUID_CANDIDATE" != "" ]; then 
            echo "S_GUID_CANDIDATE=='$S_GUID_CANDIDATE'"
        fi
        echo "GUID=='3dc15730-b36b-418f-9208-5392805096e7'"
        echo ""
    else
        echo ""
        if [ "$SB_THIS_FUNCTION_IS_MISUSED" == "t" ]; then
            echo -e "\e[31mThis function is misused.\e[39m"
        else
            echo -e "\e[31mThis function is flawed.\e[39m"
        fi
        if [ "$S_GUID_CANDIDATE" != "" ]; then 
            echo "S_GUID_CANDIDATE=='$S_GUID_CANDIDATE'"
        fi
        echo "GUID=='49ce63b6-2177-4a3b-9208-5392805096e7'"
        echo ""
    fi
    #----------------------------------------------------------------------
    # exit 1 # must NOT be called in ~/.bashrc, because 
             # exiting from the ~/.bashrc exits the session.
    #----------------------------------------------------------------------
} # func_mmmv_bashrc_subpart_wget_t1_error_message_t1

#--------------------------------------------------------------------------
if [ "$SB_WGET_EXISTS_ON_PATH" == "" ]; then
    SB_WGET_EXISTS_ON_PATH="f"
    if [ "`which wget 2> /dev/null`" != "" ]; then
        SB_WGET_EXISTS_ON_PATH="t"
    fi
else
    if [ "$SB_WGET_EXISTS_ON_PATH" != "t" ]; then
        if [ "$SB_WGET_EXISTS_ON_PATH" != "f" ]; then
            func_mmmv_bashrc_subpart_wget_t1_error_message_t1 \
                "85f24e99-a26b-49ea-9508-5392805096e7"
        fi
    fi
fi

#--------------------------------------------------------------------------
if [ "$SB_WGET_EXISTS_ON_PATH" == "t" ]; then
    MMMV_WGET_RECURSIVE_PART_NICE=" nice -n 15 "
    alias mmmv_wget_ignore_https_cert_t1="$MMMV_WGET_RECURSIVE_PART_NICE wget --no-check-certificate "
else
    if [ "$SB_WGET_EXISTS_ON_PATH" != "f" ]; then
        func_mmmv_bashrc_subpart_wget_t1_error_message_t1 \
            "65d9de36-d1f0-4658-a508-5392805096e7"
    fi
fi
