if [ "$SB_WGET_EXISTS_ON_PATH" == "t" ]; then
    #----------------------------------------------------------------------
    MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_PART_WGET_3_2_10=" \
    wget --recursive --convert-links --adjust-extension --page-requisites \
    --tries=3 --waitretry=2  --timeout=10 --user-agent=firefox "
    # -e robots=off --no-check-certificate "
    #----------------------------------------------------------------------
    MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_3_2_10=" \
    $MMMV_WGET_RECURSIVE_PART_NICE\
    $MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_PART_WGET_3_2_10 "
    #--------
    MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_TORSOCKS_3_2_10=" \
    $MMMV_WGET_RECURSIVE_PART_NICE \
    torsocks \
    $MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_PART_WGET_3_2_10 "
    #--------
    MMMV_WGET_RECURSIVE_SINGLE_DOMAIN_CMD_IGNORE_ROBOTS_TXT_3_2_10=" \
